{
  "name": "react-dashboard",
  "version": "0.1.0",
  "description": "A generic dashboard with ReactJS",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "linter": "eslint src -f html --ext .jsx --ext .js -o lint-report.html",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start-dev-server": "cross-env NODE_ENV=development node server.js",
    "start-server": "cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "cross-env": "^5.0.5",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-shallow-compare": "^15.6.0",
    "react-bootstrap": "^0.31.3",
    "react-bootstrap-daterangepicker": "^3.4.1",
    "react-dom": "^15.6.1",
    "react-highcharts": "^12.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-select": "^1.0.0-rc.10",
    "redux": "^3.6.0",
    "redux-entity": "^5.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^11.1.0",
    "json-loader": "^0.5.7",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-tabify": "0.0.24",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.0",
    "style-loader": "^0.18.2",
    "svg-react-loader": "^0.4.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  }
}
